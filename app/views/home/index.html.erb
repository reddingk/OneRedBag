<div id="wrapper">
	<div id="inner">
		<div id="carousel">
			<%= image_tag("large/img1.jpg", class: 'carousel-img', alt: 'img1', data:{ lrg_path: asset_path("large/img1.jpg"), sm_path: asset_path("small/img1.jpg")}) %>
			<%= image_tag("large/img2.jpg", class: 'carousel-img', alt: 'img2', data:{ lrg_path: asset_path("large/img2.jpg"), sm_path: asset_path("small/img2.jpg")}) %>
			<%= image_tag("large/img3.jpg", class: 'carousel-img', alt: 'img3', data:{ lrg_path: asset_path("large/img3.jpg"), sm_path: asset_path("small/img3.jpg")})%>
			<%= image_tag("large/img4.jpg", class: 'carousel-img', alt: 'img4', data:{ lrg_path: asset_path("large/img4.jpg"), sm_path: asset_path("small/img4.jpg")})%>
			<%= image_tag("large/img5.jpg", class: 'carousel-img', alt: 'img5', data:{ lrg_path: asset_path("large/img5.jpg"), sm_path: asset_path("small/img5.jpg")})%>
		</div>
		<div id="pager"></div>
	</div>
</div>

<div class="center-cards">
	<div class="col-md-4">
		<div class="card">
			<div class="card-back"><i class="fa fa-heartbeat fa-5x"></i></div>
			<div class="card-front">
				<h3>Our Mission</h3>
				<p>To Help Feed those in need.</p>
			</div>
		</div>	
	</div>
	<div class="col-md-4">
		<div class="card">
			<div class="card-back"><i class="fa fa-users fa-5x"></i></div>
			<div class="card-front">
				<h3>How To Help</h3>
				<p>Donate or Volenteer</p>
			</div>
		</div>
	</div>
	<div class="col-md-4">
		<div class="card">
			<div class="card-back"><i class="fa fa-newspaper-o fa-5x"></i></div>
			<div class="card-front">
				<h3>News</h3>
				<p>Recent News</p>
			</div>
		</div>
	</div>
</div>


